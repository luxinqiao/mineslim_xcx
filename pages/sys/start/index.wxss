#main {
    position: relative;
    padding-top: 300rpx;
    width: 100%; height: 100%;
    background:linear-gradient(334deg,rgba(90,90,209,1) 0%,rgba(133,90,209,1) 100%);
}
.logo {
    margin: 0 auto;
    width: 264rpx; height: 306rpx;
}
.bottom  {
    position: absolute;
    bottom: 0; left: 0;
    width: 100%;
}
.bottom>image {
    width: 100%;
}
.bottom>view {
    position: absolute;
    bottom: 132rpx; left: 0; right: 0;
    margin: auto;
    text-align: center;
    font-size: 36rpx; font-weight: bold;
    width: 100%;
}

.time {
    position: fixed;
    bottom: 30rpx; right: 30rpx;
    padding: 10rpx 20rpx;
    width: 100rpx;
    color: #fff; text-align: center;
    background-color: #ddd; border-radius: 20rpx;
}